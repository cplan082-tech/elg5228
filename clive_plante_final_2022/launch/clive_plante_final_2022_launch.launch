	<node name="turtlesim_node_leader" pkg="turtlesim" type="turtlesim_node" if="$(arg simulate_with_turtlesim)" />
	<node name="turtlesim_node_follower" pkg="turtlesim" type="turtlesim_node" if="$(arg simulate_with_turtlesim)" />
	<node name="drive_positive_v_w" pkg="clive_plante_final_2022" type="drive_positive_v_w.py" if="$(arg simulate_with_turtlesim)"/>
	
